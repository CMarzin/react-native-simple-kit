{
  "name": "RNSK",
  "version": "1.0.0",
  "description": "ðŸš€ Get started with React Native and Redux with no effort!",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlesMangwa/react-native-simple-kit"
  },
  "author": "Charles MANGWA <charles.mangwa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CharlesMangwa/react-native-simple-kit/issues"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "cache-clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clear",
    "test": "eslint . && flow --show-all-errors && jest",
    "test-ci": "eslint . && flow --show-all-errors && jest --coverage && cat ./__tests__/__coverage__/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "babel": {
    "presets": [
      "flow",
      "react-native"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-runtime",
          "transform-remove-console",
          "transform-flow-strip-types",
          "babel-plugin-transform-react-remove-prop-types",
          "transform-react-inline-elements",
          "transform-react-pure-class-to-function",
          "minify-constant-folding",
          "minify-dead-code-elimination"
        ]
      }
    }
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2 <4"
  },
  "rnpm": {
    "assets": [
      "src/shared/assets/"
    ]
  },
  "jest": {
    "coverageDirectory": "<rootDir>/__tests__/__coverage__",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/shared/theme/*.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "preset": "react-native",
    "rootDir": ".",
    "testRegex": "<rootDir>/__tests__/.*.test.js$",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-router-native)"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "react": "16.0.0",
    "react-native": "0.50.4",
    "react-native-modal": "^4.1.1",
    "react-native-svg": "^6.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-native": "^4.2.0",
    "react-router-navigation": "https://rrn.surge.sh/react-router-navigation-v1.0.0-rc.4.tgz",
    "react-router-redux": "next",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-persist": "^4.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-minify-constant-folding": "^0.2.0",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "0.56.0",
    "jest": "^21.2.1",
    "react-test-renderer": "16.0.0"
  }
}
